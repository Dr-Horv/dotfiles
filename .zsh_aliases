
#alias zshconfig="mate ~/.zshrc"

#Git aliases
alias gs="git status"
alias gcd="git checkout develop"
alias gd="git diff"
alias gdc="git diff --cached"

alias irc="ssh johandf@hubben.chalmers.it"
alias irkk="irc"

#Monitor manangement
alias mup="xrandr --output HDMI2 --mode 1920x1080 --above eDP1"

#Vagrant
alias vs="vagrant ssh"
alias vvs="vagrant up && vs"

#Work related
alias cdm="cd ~/work/merithon/"
alias cdma="cd ~/work/merithon/merithon-app"

#ADB
alias adbr="sudo adb kill-server && sudo adb start-server"
alias adbrev="adb reverse tcp:8081 tcp:8081"

#Nice
alias weather="curl wttr.in"
alias aliasedit="vim $HOME/.zsh_aliases && source $HOME/.zsh_aliases && rehash"
alias subl="subl3"
alias pp="pastebincl -e 1H -p"

#Bluetooth
alias btu="/usr/bin/dbus-send --system --type=method_call --dest=org.bluez /org/bluez/hci0 org.freedesktop.DBus.Properties.Set string:org.bluez.Adapter1 string:Powered variant:boolean:true"
alias btd="/usr/bin/dbus-send --system --type=method_call --dest=org.bluez /org/bluez/hci0 org.freedesktop.DBus.Properties.Set string:org.bluez.Adapter1 string:Powered variant:boolean:false"
